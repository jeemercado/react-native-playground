{
  "name": "@jeemercado/react-native-playground",
  "version": "0.0.1",
  "private": true,
  "main": "./index.js",
  "scripts": {
    "analyze": "npx react-native-bundle-visualizer --platform android --dev false --format html --reset-cache",
    "android": "cross-env react-native run-android",
    "android:connect": "adb reverse tcp:8081 tcp:8081",
    "android:clean": "cd android && ./gradlew clean",
    "android:signingReport": "cd android && ./gradlew signingReport",
    "ios:clean": "cd ios && xcodebuild clean",
    "ios": "cross-env react-native run-ios",
    "ios:logs": "npm run ios -- --verbose > ios-build.log 2>&1",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "lint:css": "npx stylelint \"./src/**/*.tsx\"",
    "lint:ts": "npx eslint --ext .ts,.tsx .",
    "lint": "npm run lint:ts --fix && npm run prettier && npm run lint:css",
    "prettier": "prettier --write \"./src/**/*.{tsx,ts}\""
  },
  "dependencies": {
